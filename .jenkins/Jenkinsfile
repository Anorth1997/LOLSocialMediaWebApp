node {
    stage('git clone source code') {
        withCredentials([usernameColonPassword(credentialsId: 'Github', variable: 'git')]) {
            sh "pwd"
            sh "ls"
            sh '''
            git clone https://"git"@github.com/Anorth1997/LOLSocialMediaWebApp.git
            '''
        }
    }

    stage('build') {
        ls
        cd LOLSocialMediaWebApp
        ls
    }

    stage('Stagging approval') {
        input "Push to stagging?"
    }

    stage('Stagging') {
        echo "stagging"
    }
}